<% if (title) { %>
    <h1><%= title %></h1>
    <% } else {
      res.redirect('/')
    } %>

<%# messages() %>

<%- listing %>

<% if (loggedin) { %>
  <form method="POST" action="/favorites/add">
    <input type="hidden" name="inv_id" value="<%= vehicle.inv_id %>" />
    <button type="submit">Add to Favorites</button>
  </form>
<% } else { %>
  <p><a href="/account/login">Log in</a> to add this vehicle to your favorites.</p>
<% } %>
